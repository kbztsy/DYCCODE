<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.springboot.dao.HeartBrainDao">
    <!-- 查询所有user -->
    <select id="GetHeartBrain" resultType="com.springboot.ModelOld.HeartBrainOld">
             SELECT
             mzzy_id,
             patient_name,
             sex_id,
             nationality_id,
             occupation_id,
             date_of_birth,
             address,
             xyqk,
             zd,
             disease_id,
             zddw,date_of_zd,
             tbdw,date_of_tb,
             person_of_tb
             from FBK_BKBZ_Record
             WHERE 1=1 AND BKLX_ID = '03' order by Date_of_TB DESC;
       </select>
    <!-- 添加数据并返回主键ID,接收主键,必须以实体类接收 -->
    <!-- keyColumn="id"对应数据库字段,keyProperty="id"对应实体类属性  -->
</mapper>
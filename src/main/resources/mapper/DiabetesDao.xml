<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.springboot.dao.DisbetesDao">
    <!-- 查询所有user -->
    <select id="GetDisbetes" resultType="com.springboot.ModelOld.DiabetesOld">
            select
            BKLX_ID,
            MZZY_ID,
            Patient_Name,
            Sex_ID,
            Nationality_ID,
            Occupation_ID,
            Date_of_Birth,
            Address,
            ZD,
            HBZ,
            Disease_ID,
            ZDDW,
            Date_of_ZD,
            TBDW,
            Date_of_TB,
            Person_of_TB
            from FBK_BKBZ_Record WHERE 1=1 AND
            BKLX_ID = '06' order by Date_of_TB DESC;
       </select>
    <!-- 添加数据并返回主键ID,接收主键,必须以实体类接收 -->
    <!-- keyColumn="id"对应数据库字段,keyProperty="id"对应实体类属性  -->
    <insert id="InsertAIDS" parameterType="com.springboot.ModelNew.AIDSNew"
            useGeneratedKeys="true" keyColumn="id" keyProperty="id">

    </insert>
</mapper>

       <!-- 查询所有user -->
       <select id="getInternal" resultType="com.springboot.ModelOld.InternalOld">
              select
                     MZZY_ID as mzzy_id,
	                 Patient_Name  as patient_name,
	                 Sex_ID  as sex_id,
	                 Date_of_Birth as  date_of_birth
             from
                     FBK_BKBZ_Record
              where
                     1=1 and BKLX_ID = '21'
                ORDER BY Date_of_TB DESC
       </select>


